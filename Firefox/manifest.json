{
  "manifest_version": 2,
  "name": "Gizoogle Tranzizzler",
  "browser_specific_settings": {
    "gecko": {
      "id": "conner.glover@proton.me",
      "strict_min_version": "48.0"
    }
  },
  "description": "An extizzle ta automate tha \"tranzizzilation\" from Gizoogle onto every last muthaf*ckin webpae you go onto.",
  "version": "1.0",
  "icons": {
    "16": "/images/icon-16.png",
    "48": "/images/icon-48.png",
    "128": "/images/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["/tranzizzler.js"]
    }
  ]
}